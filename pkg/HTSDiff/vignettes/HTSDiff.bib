@ARTICLE{Oshlack2009,
  author = {Oshlack, A. and Wakefield, M.J.},
  title = {{Transcript length bias in RNA-seq data confounds systems biology}},
  journal = {Biology Direct},
  year = {2009},
  volume = {4},
  number = {14},
  comment = {Bias is due to increased variance for shorter transcripts and higher
	sampling for long genes compared to short genes -- we need a statistical
	framework that gives similar FDR for different transcript lengths.
	# of reads is proportional to length x expression ( = more power
	to detect differential expression for longer genes). Assessed effect
	on three previous studies (Sultan et al 1008, Cloonan et al 2008,
	Marioni et al 2008)
	
	Even normalizing by gene length does not alleviate the problem (as
	gene length still affects the variance of the estimate!) -- one suggestion
	is to use a fixed length window approach},
  owner = {Andrea_2},
  timestamp = {2010.10.13}
}

