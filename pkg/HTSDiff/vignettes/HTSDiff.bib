@ARTICLE{Di2011,
  author = {Di, Yanming and Schafer, Daniel W. and Cumbie, Jason S. and Chang,
	Jeff H.},
  title = {{The NBP negative binomial model for assessing differential gene
	expression from RNA-Seq}},
  journal = {Statistical},
  year = {2011},
  volume = {10},
  pages = {1-28},
  number = {24},
  abstract = {"Our main point in the further development of statistical tools for
	differential expression from RNA-Seq, though, is that all three of
	the methods above [edgeR with trend, DESeq, NBP] improve substantially
	on the one that assumes constant dispersion parameter, that the differences
	between them are slight, and that the rapid and efficient development
	of RNA-Seq statistical tools will benefit from the competitive but
	cooperative study of teams following these different approaches."},
  comment = {NBP parameterization of the negative binomial distribution (over-parameterized:
	extra parameter alpha to describe mean-variance relationship). Good
	description of statistical challenges for RNA-seq data. Compare to
	Poisson, t-tests, limma, DESeq, edgeR. Simulations + Arabidopsis
	data.},
  owner = {Andrea_2},
  timestamp = {2011.07.05}
}

